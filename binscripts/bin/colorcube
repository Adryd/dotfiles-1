#!/bin/bash
rgb() {
    r=$1
    g=$2
    b=$3
    echo $((16+$r*36+$g*6+$b))
}
for r in $(seq 0 5) ; do
    for g in $(seq 0 5) ; do
        for b in $(seq 0 5) ; do
            echo -en "\e[38;5;$(rgb $r $g $b)m██\e[0m"
        done
        #echo
    done
    echo
done
for i in $(seq 232 255) ; do 
    echo -en "\e[38;5;${i}m██\e[0m"
done
echo
