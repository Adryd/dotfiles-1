"*":
  core:
    audioBeep: false
    autoHideMenuBar: true
    automaticallyUpdate: false
    disabledPackages: [
      "autocomplete-html"
      "autocomplete-snippets"
      "background-tips"
      "bookmarks"
      "encoding-selector"
      "git-diff"
      "go-to-line"
      "language-clojure"
      "link"
      "styleguide"
      "spell-check"
      "welcome"
      "archive-view"
      "autocomplete-atom-api"
      "autocomplete-css"
      "autocomplete-plus"
      "snippets"
      "vim-mode"
    ]
    ignoredNames: [
      ".git"
      ".hg"
      ".svn"
      ".DS_Store"
      "._*"
      "Thumbs.db"
      "node_modules"
      "bower_components"
    ]
    projectHome: "/home/cheesy/dev/prj"
    reopenProjectMenuCount: 30
    telemetryConsent: "limited"
    themes: [
      "one-dark-ui"
      "ariake-dark-syntax"
    ]
    warnOnLargeFileLimit: 10
  editor:
    fontFamily: "Iosevka"
    fontSize: 18
    lineHeight: 1.3
    scrollPastEnd: true
    showIndentGuide: true
    softWrap: true
    softWrapAtPreferredLineLength: true
    zoomFontWhenCtrlScrolling: false
  "exception-reporting":
    userId: "2b0a651f-3b87-c135-312a-bae015b05497"
  "file-icons":
    coloured: false
  "language-babel": {}
  "one-dark-ui":
    layoutMode: "Compact"
  "tree-view":
    hideIgnoredNames: true
    hideVcsIgnoredFiles: true
    squashDirectoryNames: true
  welcome:
    showOnStartup: false
