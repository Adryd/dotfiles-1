! cursor
Xcursor.theme: Vanilla-DMZ

! xft
Xft.dpi: 96
Xft.rgba: rgb
Xft.hinting: 1
Xft.antialias: 1
Xft.lcdfitler: lcddefault
Xft.hintstyle: hintslight

! urxvt
urxvt.scrollBar: false
! urxvt.font: xft:Fantasque Sans Mono:pixelsize=16
! urxvt.font: xft:Iosevka:pixelsize=17
! urxvt.font: xft: Office Code Pro:pixelsize=15
! urxvt.font: xft:PragmataPro Mono:pixelsize=12
urxvt.font: xft:PragmataPro Mono:pixelsize=18
! urxvt.font: xft:PxPlus IBM BIOS:pixelsize=8:antialias=false
! urxvt.font: xft:PxPlus IBM EGA8:pixelsize=15:antialias=false
! urxvt.font: -lucy-tewi-medium-r-normal--11-90-100-100-c-60-iso10646-*
! urxvt.boldFont: -lucy-tewi-bold-r-normal--11-80-100-100-c-60-iso10646-*
! urxvt.boldFont: xft:Essential PragmataPro:style=bold:pixelsize=13
urxvt.letterSpace: -2
urxvt.intensityStyles: false

st.termname:     st-256color
! st.font:         Akkurat-Mono:pixelsize=17
! st.font:         PragmataPro Mono:pixelsize=17
st.font:         Iosevka:pixelsize=17
st.borderpx:     2
st.cursorshape:  2
st.cursorthickness:  2
st.bold_font:    1
st.xfps:         60
st.actionfps:    60
st.chscale:      1.0
st.cwscale:      0.9
st.blinktimeout: 800
st.bellvolume:   0
st.opacity:      255

*.foreground: #BCBCBC
*.background: #262626
*.color0:     #1C1C1C
*.color8:     #444444
*.color1:     #AF5F5F
*.color9:     #FF8700
*.color2:     #5F875F
*.color10:    #87AF87
*.color3:     #87875F
*.color11:    #FFFFAF
*.color4:     #5F87AF
*.color12:    #8FAFD7
*.color5:     #5F5F87
*.color13:    #8787AF
*.color6:     #5F8787
*.color14:    #5FAFAF
*.color7:     #6C6C6C
*.color15:    #FFFFFF
